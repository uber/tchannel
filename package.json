{
  "name": "tchannel",
  "description": "network multiplexing and framing protocol for RPC or parser drag racing",
  "author": "mranney@uber.com",
  "version": "1.2.3",
  "scripts": {
    "test": "node test/index.js",
    "cover": "istanbul cover --print detail --report html -- test/index.js",
    "view-cover": "opn coverage/index.html",
    "check-licence": "uber-licence --dry",
    "add-licence": "uber-licence"
  },
  "repository": {
    "type": "git",
    "url": "gh:uber/tchannel"
  },
  "dependencies": {
    "farmhash": "^0.2.0",
    "metrics": "^0.1.8"
  },
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jshint": "^2.5.6",
    "opn": "^1.0.1",
    "pre-commit": "0.0.9",
    "tape": "^3.0.3",
    "time-mock": "^0.1.2",
    "uber-licence": "git+ssh://git@github.com:uber/uber-licence"
  },
  "pre-commit": [
    "test",
    "check-licence"
  ],
  "pre-commit.silent": true
}
