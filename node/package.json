{
  "name": "tchannel",
  "description": "network multiplexing and framing protocol for RPC or parser drag racing",
  "author": "mranney@uber.com",
  "version": "2.0.0-beta18",
  "scripts": {
    "lint": "jshint .",
    "test": "npm run lint -s && npm run cover -s && istanbul check-coverage",
    "benchmark": "echo '!!! DEPRECATED: Better to just run `node benchmark` directly' >&2; node benchmarks",
    "take-benchmark": "./benchmarks/take.sh",
    "cover": "istanbul cover --report html --print none test/index.js | faucet && istanbul report text",
    "view-cover": "opn coverage/index.html",
    "check-licence": "uber-licence --dry",
    "add-licence": "uber-licence"
  },
  "main": "channel",
  "repository": {
    "type": "git",
    "url": "git@github.com:uber/tchannel"
  },
  "dependencies": {
    "bufrw": "git://github.com/raynos/bufrw.git#upgrade-error",
    "crc": "^3.2.1",
    "error": "^6.2.0",
    "farmhash": "^0.2.0",
    "json-stringify-safe": "^5.0.0",
    "readable-stream": "1.0.33",
    "ready-signal": "^1.1.1",
    "rezult": "^1.0.0",
    "run-parallel": "^1.1.1",
    "sse4_crc32": "^3.1.0",
    "thriftify": "1.0.0-alpha13",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "ansi-color": "^0.2.1",
    "async": "^0.9.0",
    "chalk": "^1.0.0",
    "child_pty": "^0.5.1",
    "debug-logtron": "3.2.0",
    "duplexer": "^0.1.1",
    "faucet": "0.0.1",
    "istanbul": "^0.3.13",
    "jshint": "^2.5.6",
    "ldjson-stream": "^1.2.1",
    "logtron": "^8.1.0",
    "metrics": "^0.1.8",
    "minimist": "^1.1.0",
    "once": "^1.3.1",
    "opn": "^1.0.1",
    "replr": "^1.0.4",
    "run-series": "^1.0.2",
    "safe-json-parse": "^4.0.0",
    "split2": "^0.2.1",
    "tape": "^4.0.0",
    "through2": "^0.6.3",
    "time-mock": "^0.1.2",
    "uber-licence": "^1.1.0"
  },
  "pre-commit": [],
  "pre-commit.silent": true
}
